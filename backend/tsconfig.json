{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"types": ["node", "jest"]
	}
}
